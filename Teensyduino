int ledPin = 13; // Teensy has a built in LED here 

void setup() {
   pinMode(ledPin, OUTPUT); // initialize pin as an output
}

void loop() {
   digitalWrite(ledPin, HIGH);// light on high 
   delay (1000);              // pause, can comment this out if you just want to display the led 
   digitalWrite(ledPin, LOW); // light on low
   delay(1000):               // pause
}
